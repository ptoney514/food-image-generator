<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.localai.service</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Users/pernelltoney/LocalAI/local-ai</string>
        <string>--models-path=/Users/pernelltoney/LocalAI/models</string>
